config_version=5

[application]

config/name="Team Game Template"
config/description="Team Game Platform by Yni"
config/version="0.2.0"
config/tags=PackedStringArray("multiplayer", "team", "template")
run/main_scene="res://Scenes/Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="TGPY_Project"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://Assets/intro.png"
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

GDsh="*res://GDsh/GDsh.gd"
Settings="*res://Scripts/Settings.gd"

[display]

